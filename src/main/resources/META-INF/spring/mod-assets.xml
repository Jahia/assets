<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean class="org.jahia.services.render.StaticAssetMapping">
        <property name="mapping">
            <map>
                <entry key="jquery.js" value="jquery${jahia.jquery.version}.min.js jquery.jahia.min.js"/>
                <entry key="jquery.min.js" value="jquery${jahia.jquery.version}.min.js jquery.jahia.min.js"/>
                <entry key="jquery.jahia.js" value="jquery.jahia.min.js"/>

                <entry key="ckeditor/ckeditor.js" value="ckeditor.js"/>
                <entry key="ckeditor/adapters/jquery.js" value="adapters/jquery.js"/>
            </map>
        </property>
    </bean>

</beans>